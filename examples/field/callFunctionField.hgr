def cFoo
    oBar = null
end

# the following procedure takes the Function prototype as poThis
def pBar(pnA, pnB)
    println("bar")
    println(pnA)
    println(pnB)
end

let oFoo = new cFoo()
oFoo.oBar = pBar # this changes the Function prototype of pBar from Function to instance of cFoo

oFoo.oBar(1, 2) # here pBar is owned by oFoo, so it is called as a method of oFoo