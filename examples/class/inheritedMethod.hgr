def cFoo
    pInFoo
        pPrintLn("in foo")
    end
end

def cBar as cFoo
    pInBar
        pPrintLn("in bar")
    end
end

def cBaz as cBar
    pInBaz
        pPrintLn("in baz")
    end
end

let oBaz = new cBaz()
oBaz.pInFoo() # in foo
oBaz.pInBar() # in bar
oBaz.pInBaz() # in baz