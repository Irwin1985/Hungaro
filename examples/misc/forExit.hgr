"""
 For Loop Example using exit statement

 Author: Irwin Rodr√≠guez <rodriguez.irwin@gmail.com>
 Date: 2023-02-23
"""
let nExternalI = 0
for nI = 1 to 10
    if nI == 5
        nExternalI = nI
        exit
    end
end

pAssert(nExternalI == 5, "nExternalI should be 5")

# final assert to automatically check if all tests passed
pAssert(true, "This test should always pass")