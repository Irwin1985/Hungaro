let nRows = 7

for nI = 1 to nRows * 2    
    for nJ = 1 to nRows * 2
        if (nJ == nRows or nI == nRows)
            pPrint("*")
        else
            pPrint(" ")
        end
    end
    pPrintLn(" ")
end

# final assert to automatically check if all tests passed
pAssert(true, "This test should always pass")