declare lfValidateData(pmData)
    guard pmData.contains("name") and pmData.contains("age") and pmData.contains("address") else                
        return false
    end
    return true
end

declare lpTest(pmData)
    guard lfValidateData(pmData) else
        println("Missing data")
        exit
    end
    println(pmData.name)
    println(pmData.age)
    println(pmData.address)
end

lpTest({name: "Irwin", age: 37})