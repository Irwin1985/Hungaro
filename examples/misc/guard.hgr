def fValidateData(pmData)
    guard pmData.contains("name") and pmData.contains("age") and pmData.contains("address") else                
        return false
    end
    return true
end

def pTest(pmData)
    guard fValidateData(pmData) else
        pPrintLn("Missing data")
        exit
    end
    pPrintLn(pmData.name)
    pPrintLn(pmData.age)
    pPrintLn(pmData.address)
end

pTest({name: "Irwin", age: 37})