let nRows = fVal(fReadLn("Enter K Shape Alphabet Pattern Rows = "))

fPrint("====K Shape Alphabet Pattern====\n")

for nI = nRows to 1 step -1
    let nAlphabet = 65
    for nJ = 1 to nRows
        fPrint(fChr(nAlphabet + nJ))
    end
    fPrint(" ")
end