let nRows = val(readln("Enter K Shape Alphabet Pattern Rows = "))

print("====K Shape Alphabet Pattern====\n")

for nI = nRows to 1 step -1
    let nAlphabet = 65
    for nJ = 1 to nRows
        print(chr(nAlphabet + nJ))
    end
    print(" ")
end