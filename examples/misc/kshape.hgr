let nRows = fVal(fReadLn("Enter K Shape Alphabet Pattern Rows = "))

pPrint("====K Shape Alphabet Pattern====\n")

for nI = nRows to 1 step -1
    let nAlphabet = 65
    for nJ = 1 to nRows
        pPrint(fChr(nAlphabet + nJ))
    end
    pPrint(" ")
end

# final assert to automatically check if all tests passed
pAssert(true, "This test should always pass")