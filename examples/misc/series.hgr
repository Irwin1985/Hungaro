# Add the first 100 terms of a simple series
for nI = 1 to 100
    let nValue = (1 + 1 / nI) ^ nI
    pPrintLn(nI, " ", nValue)
end

# final assert to automatically check if all tests passed
pAssert(true, "This test should always pass")