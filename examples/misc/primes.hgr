let nUpper = 50

for nNum = 0 to nUpper
    # all prime numbers are greater than 1
    if nNum > 1
        for nI = 2 to nNum
            println("nI = " & nI)
            if nNum % nI == 0
                exit
            end
        end
        println(nNum)
    else
        println(nNum)
    end
end