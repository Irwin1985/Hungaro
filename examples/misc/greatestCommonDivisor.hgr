"""
 Greatest Common Divisor

 Author: Irwin Rodr√≠guez <rodriguez.irwin@gmail.com>
 Date: 2023-02-23
"""
println("Enter two positive integers")

let nN1 = val(readln("Enter the first integer: "))
let nN2 = val(readln("Enter the second integer: "))

while nN1 != nN2
    if nN1 > nN2
        nN1 -= nN2
    else
        nN2 -= nN1
    end
end
println("The greatest common divisor is: ", nN1)
