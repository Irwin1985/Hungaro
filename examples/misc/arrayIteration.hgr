let aNotes = []

# populate the array using a loop
for nI = 1 to 10
    aNotes.push(nI)
end

# iterate over the array
for nI = 0 to aNotes.len()-1
    println(aNotes[nI])
end

# shuffle the array
aNotes.shuffle()
println(aNotes)