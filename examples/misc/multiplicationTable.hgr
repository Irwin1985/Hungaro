for nT = 1 to 10
    pPrintLn("Multiplication table of " & nT)
    for nJ = 1 to 10
        pPrintLn("" & nT & " x " & nJ & " = " & nT * nJ)
    end
end

# final assert to automatically check if all tests passed
pAssert(true, "This test should always pass")