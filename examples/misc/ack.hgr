def fAck(pnX, pnY)
    if pnX == 0
        return pnY + 1
    else
        if pnY == 0
            return fAck(pnX - 1, 1)
        else
            return fAck(pnX - 1, fAck(pnX, pnY - 1))
        end
    end
end

# Call the function
pPrintLn(fAck(3, 5)) # 253

# final assert to automatically check if all tests passed
