"""

let nSize = 5
let aRow = [] * nSize = 0
let aMatrix = [] * nSize = aRow

for nI = 0 to nSize - 1
    for nJ = 0 to nSize - 1
        aMatrix[nI][nJ] = 1 / ((nI + 1) + (nJ + 1) - 1)
    end    
end

println(aMatrix)