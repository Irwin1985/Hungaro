let oQueue = fQueue()

for nI = 1 to 10
    oQueue.pEnqueue("Element " & nI)
end

# Peek at the top of the queue
pPrintLn(oQueue.fPeek())

# Dequeue from queue
pPrintLn(oQueue.fDequeue())

# print the number of elements in the queue
pPrintLn(oQueue.fLen())

# print the queue
pPrintLn(oQueue)

# clear the queue
oQueue.pClear()