            oZoo.fBanana() + ;
            oZoo.fTuna()   + ;
            oZoo.fHay()    + ;
            oZoo.fGrass()  + ;
            oZoo.fMouse()
end

println("Sum: " & nSum)
println("Time: " & tick(nStart) & "ms")