def cZoo
    pInit
        poThis.nAarvark = 1
        poThis.nBaboon = 1
        poThis.nCat = 1
        poThis.nDonkey = 1
        poThis.nElephant = 1
        poThis.nFox = 1
    end

    fAnt
        return poThis.nAarvark
    end

    fBanana
        return poThis.nBaboon
    end

    fTuna
        return poThis.nCat
    end

    fHay
        return poThis.nDonkey
    end

    fGrass
        return poThis.nElephant
    end

