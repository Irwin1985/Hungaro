let sA = "a", sB = "b", sC = "c"

# Assignment is right associative
sA = sB = sC

assert(sA == "c", "sA is not equal to 'c'")
assert(sB == "c", "sB is not equal to 'c'")
assert(sC == "c", "sC is not equal to 'c'")