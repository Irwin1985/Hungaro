declare loF

declare lpF1
    declare lsA = "a"
    declare lpF2
        declare lsB = "b"
        declare lpF3
            declare lsC = "c"
            declare lpF4
                println(lsA)
                println(lsB)
                println(lsC)
            end
            loF = lpF4
        end
        lpF3()
    end
    lpF2()
end

lpF1()
loF()