declare lfFib(pnSerie)
    if pnSerie < 2
        return pnSerie
    end
    return lfFib(pnSerie - 1) + lfFib(pnSerie - 2)
end

declare lnStart = tick()
assert(lfFib(35) == 9227465, "Fibbonacci sequence failed")
println(tack(lnStart))