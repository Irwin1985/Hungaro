declare lnRows = val(readln("Enter K Shape Alphabet Pattern Rows = "))

print("====K Shape Alphabet Pattern====\n")

for lnI = lnRows to 1 step -1
    declare lnAlphabet = 65
    for lnJ = 1 to lnRows
        print(chr(lnAlphabet + lnJ))
    end
    print(" ")
end