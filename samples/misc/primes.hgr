declare lnUpper = 50

for lnNum = 0 to lnUpper
    # all prime numbers are greater than 1
    if lnNum > 1
        for lnI = 2 to lnNum
            println("lnI = " & lnI)
            if lnNum % lnI == 0
                exit
            end
        end
        println(lnNum)
    else
        println(lnNum)
    end
end