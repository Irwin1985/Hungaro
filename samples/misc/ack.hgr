declare lfAck(pnX, pnY)
    if pnX == 0
        return pnY + 1
    else
        if pnY == 0
            return lfAck(pnX - 1, 1)
        else
            return lfAck(pnX - 1, lfAck(pnX, pnY - 1))
        end
    end
end

# Call the function
pritn(lfAck(3, 5)) # 253