"""
 Greatest Common Divisor

 Author: Irwin Rodr√≠guez <rodriguez.irwin@gmail.com>
 Date: 2023-02-23
"""
println("Enter two positive integers")

declare lnN1 = val(readln("Enter the first integer: "))
declare lnN2 = val(readln("Enter the second integer: "))

while lnN1 != lnN2
    if lnN1 > lnN2
        lnN1 -= lnN2
    else
        lnN2 -= lnN1
    end
end
println("The greatest common divisor is: ", lnN1)
