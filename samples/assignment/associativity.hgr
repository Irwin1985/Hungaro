declare lsA = "a", lsB = "b", lsC = "c"

# Assignment is right associative
lsA = lsB = lsC

assert(lsA == "c", "lsA is not equal to \'c\'")
assert(lsB == "c", "lsB is not equal to \'c\'")
assert(lsC == "c", "lsC is not equal to \'c\'")