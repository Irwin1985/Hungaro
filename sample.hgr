def fFib(pnSerie)
    if pnSerie < 2
        return pnSerie
    end
    return fFib(pnSerie - 1) + fFib(pnSerie - 2)
end

let nStart = fTick()
pAssert(fFib(35) == 9227465, "Fibbonacci sequence failed")
pPrintLn(fTack(nStart))