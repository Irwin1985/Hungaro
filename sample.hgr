declare lcPerson
    sName = ""
    nAge = 0

    pInit(psName, pnAge)
        sName = psName
        nAge = pnAge
    end

    pWalk
        println("Walking")
    end
end

declare lcGerente as lcPerson
    pInit(psName, pnAge, pnSalary)
        super(psName, pnAge)
        nSalary = pnSalary
    end

end

declare loPerson = new lcGerente("John", 20, 1500)
println(loPerson.properties)